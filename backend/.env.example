# Environment Configuration Template
# Copy this file to .env and fill in your actual values
# IMPORTANT: Never commit the actual .env file with real credentials

# Environment
NODE_ENV=development

# Server Configuration
PORT=5000

# MongoDB Connection
MONGO_URI=mongodb+srv://username:password@cluster.mongodb.net/databaseName

# JWT Secret (use a strong, random string in production)
JWT_SECRET=your_super_secret_jwt_key_here

# Nutrition API (RapidAPI)
NUTRITION_API_HOST=nutrition-calculator.p.rapidapi.com
NUTRITION_API_KEY=your_rapidapi_key_here

# Google Document AI Configuration
GOOGLE_BASE_URI=https://us-documentai.googleapis.com/v1/projects/YOUR_PROJECT_ID/locations/YOUR_LOCATION/processors/YOUR_PROCESSOR_ID:process
DOCUMENT_PROJECT_ID=your_google_project_id
DOCUMENT_PROJECT_LOCATION=us
DOCUMENT_PROCESSOR_ID=your_processor_id

# CORS Configuration (comma-separated list of allowed origins for production)
# Example: https://yourapp.azurewebsites.net,https://www.yourdomain.com
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:5000
